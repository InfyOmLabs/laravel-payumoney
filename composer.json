{
    "name": "infyomlabs/laravel-payumoney",
    "type": "library",
    "description": "Laravel Payumoney Integration Library",
    "keywords": [
        "laravel",
        "payu-money",
        "payu",
        "payment",
        "payent-gateway"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mitul Golakiya",
            "email": "me@mitul.me"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "illuminate/support": "^9.0|^8.0|^7.0|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit": "^8.5.8|^9.5"
    },
    "autoload": {
        "psr-4": {
            "InfyOmLabs\\Payu\\" : "src/"
        }
    }   ,
    "extra": {
        "laravel" : {
                "providers": [
                    "InfyOmLabs\\Payu\\PayuMoneyAppServiceProvider"
                ]
        }
